{
  "name": "RadOrderPad",
  "short_name": "RadOrderPad",
  "description": "HIPAA-compliant radiology order validation system",
  "start_url": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay"],
  "background_color": "#ffffff",
  "theme_color": "#2563eb",
  "orientation": "any",
  "categories": ["medical", "healthcare", "productivity"],
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "icons": [],
  "screenshots": [],
  "scope": "/",
  "id": "radorderpad-radiologist-productivity-tool",
  "lang": "en-US",
  "dir": "ltr",
  "prefer_related_applications": false,
  "related_applications": [],
  "shortcuts": [
    {
      "name": "New Order",
      "short_name": "New Order",
      "description": "Start a new radiology order",
      "url": "/new-order",
      "icons": []
    }
  ],
  "edge_side_panel": {
    "preferred_width": 480
  },
  "handle_links": "preferred",
  "iarc_rating_id": "",
  "share_target": {
    "action": "/share-target",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "text/json": [".json"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+radorder",
      "url": "/protocol-handler?url=%s"
    }
  ],
  "apple-mobile-web-app-capable": "yes",
  "apple-mobile-web-app-status-bar-style": "black-translucent",
  "apple-mobile-web-app-title": "RadOrderPad"
}